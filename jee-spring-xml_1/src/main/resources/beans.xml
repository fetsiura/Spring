<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="helloWorld" class="pl.jaro.beans.HelloWorld">
        <constructor-arg name="message" value="Witaj Spring"/>
    </bean>
    <bean id="helloWorld2" class="pl.jaro.beans.HelloWorld">
        <property name="message" value="Witaj Spring - by setter"/>
    </bean>
    <bean id="emailService" class="pl.jaro.beans.EmailService"/>
</beans>